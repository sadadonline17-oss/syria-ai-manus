{
  "name": "backend",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "dev:tunnel": "concurrently \"bun run dev\" \"node scripts/start-ngrok-cli.js\" \"node scripts/setup-ngrok-env.js\"",
    "build": "bun run build.js",
    "start": "node dist/index.js",
    "lint": "eslint \"**/*.{js,ts}\" && prettier -c \"**/*.{js,ts,json}\"",
    "format": "eslint \"**/*.{js,ts}\" --fix && prettier \"**/*.{js,ts,json}\" --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^4.0.52",
    "@ai-sdk/anthropic": "^3.0.39",
    "@ai-sdk/cohere": "^3.0.19",
    "@ai-sdk/deepseek": "^2.0.18",
    "@ai-sdk/google": "^3.0.22",
    "@ai-sdk/mistral": "^3.0.19",
    "@ai-sdk/openai": "^3.0.26",
    "@ai-sdk/xai": "^3.0.49",
    "@hono/zod-validator": "^0.7.4",
    "@octokit/rest": "^22.0.1",
    "@openrouter/ai-sdk-provider": "^2.1.1",
    "@supabase/supabase-js": "^2.75.0",
    "ai": "^6.0.78",
    "hono": "^4.9.10",
    "ollama-ai-provider": "^1.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.25.1",
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "ngrok": "^5.0.0-beta.2",
    "nodemon": "^3.1.9",
    "prettier": "^3.6.2"
  }
}
